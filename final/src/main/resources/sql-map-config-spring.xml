<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<!-- 오라클 컬럼이 first_name 를 VO firstName 처럼 카멜케이스로 변환 -->
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>
	<typeAliases>
		<typeAlias type="co.team.apt.emp.EmpVO" alias="emp"/> 
		<typeAlias type="co.team.apt.person.vo.ManagerVo" alias="manager"/>
		<typeAlias type="co.team.apt.person.vo.ResidentVo" alias="resident"/>
		<typeAlias type="co.team.apt.community.vo.BcommentVo" alias="bcomment"/>
		<typeAlias type="co.team.apt.community.vo.BoardVo" alias="board"/>
		<typeAlias type="co.team.apt.payment.vo.PaymentVo" alias="payment"/>
		<typeAlias type="co.team.apt.vote.vo.BoardVoteVo" alias="vote"/>
		<typeAlias type="co.team.apt.vote.vo.CandidateVo" alias="candidate"/>
		<typeAlias type="co.team.apt.esey.vo.VisitCarVo" alias="vCar"/>
	</typeAliases>

</configuration>
